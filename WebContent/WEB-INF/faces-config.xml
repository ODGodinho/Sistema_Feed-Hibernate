		<navigation-case>
			<from-outcome>pinguco</from-outcome>
			<to-view-id>/pages/pinguco.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<!-- ################################ Menu ######################### -->

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
